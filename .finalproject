    methods (Access = private)
        
        function [] = waitForPlayerOneData(app)
            playerWhoSentMsg = 0;
            
            while playerWhoSentMsg ~= 1
                data = thingSpeakRead(app.channelID,'readKey',app.readAPI);
                playerWhoSentMsg = data(4);
                pause(app.readDelay);
            end
         
            app.diceRoll_2 = data(7);
        app.diceRoll = data(1);  
            
        end
        
        function [] = waitForPlayerTwoData(app)
            playerWhoSentMsg = 0;
            
            while playerWhoSentMsg ~= 2
                data = thingSpeakRead(app.channelID,'readKey',app.readAPI);
                playerWhoSentMsg = data(4);
                pause(app.readDelay);
            end
            
        
            
        end
        
       
    end


